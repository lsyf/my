<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loushuiyifan.report.dao.RptFundsFeeStatusDAO">
	<select id="listFundsFee" resultType="com.loushuiyifan.report.vo.FundsStatusVO" >
		select report_id||'-'||report_name as reportName,
			status,
			voucher_code as voucherCode			
			from v_step_balance  
			where acct_month=#{month}
		    <if test='reportId !="0" '>
			and REPORT_ID = #{reportId}
			</if>
		
	</select> 
	
	<select id="quitData" parameterType="com.loushuiyifan.report.dto.SPDataDTO"
            statementType="CALLABLE">
        {call PKG_RPT_BALANCE.p_quit_audit(
         #{userId,mode=IN,jdbcType=NUMERIC},
         #{month,mode=IN,jdbcType=VARCHAR},
         #{reportId,mode=IN,jdbcType=VARCHAR},
         #{rtnCode,mode=OUT,jdbcType=NUMERIC},
         #{rtnMsg,mode=OUT,jdbcType=VARCHAR})}
    </select>
	
</mapper>